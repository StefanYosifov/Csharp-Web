@{
    ViewData["Title"] = "The numbers to N";
}

<form asp-controller="Home" asp-action="NumbersToN" method="post">
    <div class="row">
        <div class="col">
            <input type="number" min="0" class="count"/>
        </div>
        <div class="col-2">
            <button> Submit </button>
        </div>
    </div>
</form>